{
	"kind": "List",
	"apiVersion": "v1",
	"metadata": {
		"name": "springboot-sti"
	},
	"items": [{
			"kind": "BuildConfig",
			"apiVersion": "v1",
			"metadata": {
				"name": "springboot-sti"
			},
			"spec": {
				"triggers": [{
					"type": "GitHub",
					"github": {
						"secret": "secret"
					}
				}, {
					"type": "Generic",
					"generic": {
						"secret": "secret"
					}
				}, {
					"type": "ImageChange",
					"imageChange": {}
				}],
				"source": {
					"type": "Git",
					"git": {
						"uri": "https://github.com/promuks/cif-service.git",
						"ref": "master"
					},
					"contextDir": "springboot-sti"
				},
				"strategy": {
					"type": "Docker",
					"dockerStrategy": {
						"from": {
							"kind": "ImageStreamTag",
							"name": "rhel7.2:latest"
						}
					}
				},
				"output": {
					"to": {
						"kind": "ImageStreamTag",
						"name": "springboot-sti:latest"
					}
				},
				"resources": {}
			}
		}, {
			"kind": "ImageStream",
			"apiVersion": "v1",
			"metadata": {
				"name": "rhel7.2"
			},
			"spec": {
				"dockerImageRepository": "registry.access.redhat.com/rhel7.2:latest"
			}
		}, {
			"kind": "ImageStream",
			"apiVersion": "v1",
			"metadata": {
				"name": "springboot-sti"
			},
			"spec": {
				"dockerImageRepository": "",
				"tags": [{
					"name": "latest",
					"annotations": {
						"description": "Spring Boot STI image",
						"iconClass": "icon-jboss",
						"tags": "builder,springboot,java,maven",
						"supports": "springboot,java:8,maven:3",
						"version": "1.0"
					}
				}]
			}
		}

	]
}
